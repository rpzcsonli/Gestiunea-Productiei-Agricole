
<h1>Sterge Plante</h1>
<form method="post">
    @foreach (var evt in ViewBag.events)
    {
        <div class="form-group">
            <label>
                <span>@evt.Nume</span>
                <input type="checkbox" name="plantaId" value="@evt.CodPlanta" />
            </label>
        </div>
    }
    <input type="submit" name="Stergere" class="btn btn-danger" />
</form>


