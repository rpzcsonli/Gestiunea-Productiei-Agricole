                            <a href="@Url.Action("Index", new { angajatSelectat, registruSelectat,filtrareData=Model.filtrareData ,dataInceput=Model.DataInceput.ToString("yyyy-MM-dd") ,dataSfarsit=Model.DataSfarsit.ToString("yyyy-MM-dd"), sortOrder = currentSort == "DataAplicare" ? "DataAplicare_desc" : "DataAplicare" })">
                                    @if (currentSort != "DataAplicare_desc")
                                    {
                                        <i class="bi bi-sort-up"></i>
                                    }
                                    else
                                    {
                                        <i class="bi bi-sort-down"></i>
                                    }
                                </a>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                    @foreach (var registru in Model.RegistruTratamente)
                        {
                            total += registru.Suprafata;
                            <tr>
                                <td>@registru.CodTratamentAplicat</td>
                                <td>@registru.CodParcela - @registru.Parcela.Locatie</td>
                                <td>@registru.Daunatori.Denumire</td>
                                <td>@registru.Daunatori.Tratament.Denumire</td>
                                <td>@registru.Suprafata &#x33A1;</td>
                                <td>@registru.DataAplicare.ToString("dd/MM/yyyy")</td>
                            </tr>
                        }
                    <td></td>
                    <td></td>
                    <td></td>
                    <td>Total Suprafata</td>
                    <td>@total &#x33A1;</td>
                    <td></td>
                    </tbody>
                </table>
            }
        
    }
    else
    {
        <p>Nu exista date.</p>
    }
</div>
