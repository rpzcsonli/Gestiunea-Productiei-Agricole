                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title text-center">Parcela @parcela.CodParcela</h5>
                                <table class="table">
                                    <tr>
                                        <td>Locatie: @parcela.Locatie</td>
                                        <td>Tip: @parcela.Tip</td>
                                    </tr>
                                    <tr>
                                        <td>Suprafata: @parcela.Suprafata &#x33A1;</td>
                                        <td>Numar Plante: @parcela.NumarPlante</td>
                                    </tr>
                                </table>
